{
  "name": "v-selectpage",
  "description": "SelectPage for Vue, list or table view of pagination, use tags for multiple selection, i18n and server side resources supports",
  "version": "3.0.0-beta.1",
  "author": "TerryZ <terry5@foxmail.com>",
  "type": "module",
  "files": [
    "/dist",
    "/types"
  ],
  "main": "./dist/v-selectpage.umd.cjs",
  "module": "./dist/v-selectpage.js",
  "exports": {
    ".": {
      "import": "./dist/v-selectpage.js",
      "require": "./dist/v-selectpage.umd.cjs"
    }
  },
  "typings": "types/index.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "dts": "vue-tsc --declaration --emitDeclarationOnly"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TerryZ/v-selectpage.git"
  },
  "keywords": [
    "front-end",
    "javascript",
    "vue",
    "selector",
    "tableview",
    "i18n",
    "server-side",
    "pagination",
    "tags",
    "multiple"
  ],
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "v-dropdown": "3.0.0-beta.1",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.15",
    "bootstrap": "^5.3.1",
    "eslint": "^8.47.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.27",
    "sass": "^1.65.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vitest": "^0.34.1",
    "vue-router": "^4.2.4",
    "vue-tsc": "^1.8.8"
  }
}
